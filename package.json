{
  "name": "elm-doctest",
  "version": "0.3.7",
  "description": "doctest for elm",
  "main": "./src/bridge.js",
  "bin": {
    "elm-doctest": "./bin/cli.js"
  },
  "devDependencies": {
    "elm": "^0.19.0",
    "npm-check": "^5.8.0",
    "shelljs": "^0.8.1",
    "standard": "^11.0.1",
    "yarn": "^1.9.4"
  },
  "scripts": {
    "prebuild": "yarn install",
    "dist": "yarn build",
    "build": "elm make src/Main.elm --output ./src/elm.js",
    "check": "npm-check -s -p",
    "lint": "standard src/bridge.js src/main.js src/util.js",
    "pretest": "yarn build && yarn check && yarn lint",
    "test0": "node ./test/run.js",
    "test": "yarn test0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tshm/elm-doctest.git"
  },
  "author": "Toshihide Satake <2sm@csc.jp>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tshm/elm-doctest/issues"
  },
  "homepage": "https://github.com/tshm/elm-doctest#readme",
  "keywords": [
    "cli",
    "tool",
    "elm",
    "doctest"
  ],
  "dependencies": {
    "chokidar": "^2.0.4",
    "minimist": "^1.2.0"
  }
}
