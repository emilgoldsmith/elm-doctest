{
  "name": "elm-doctest",
  "version": "0.2.2",
  "description": "doctest for elm",
  "main": "./distribution/index.js",
  "bin": {
    "elm-doctest": "./cli.js"
  },
  "devDependencies": {
    "elm": "^0.18.0",
    "eslint": "^3.4.0",
    "npm-check": "^5.4.0"
  },
  "scripts": {
    "prebuild": "npm install && elm-package install --yes",
    "build": "elm-make --yes --warn src/Main.elm --output ./distribution/index.js",
    "check": "npm-check -s -p",
    "lint": "eslint src/bridge.js",
    "pretest": "npm run build && npm run check && npm run lint",
    "cli": "node ./cli.js",
    "test": "npm run cli -- src/DocTest.elm test/ParamTest.elm test/ParamTest2.elm  test/RandomTest.elm test/TestData.elm test/TestView.elm",
    "test:fail": "npm run cli -- test/TestView.elm test/TestData/TestFail.elm || echo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tshm/elm-doctest.git"
  },
  "author": "Toshihide Satake <2sm@csc.jp>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tshm/elm-doctest/issues"
  },
  "homepage": "https://github.com/tshm/elm-doctest#readme",
  "keywords": [
    "cli",
    "tool",
    "elm",
    "doctest"
  ],
  "dependencies": {
    "chokidar": "^1.6.1",
    "minimist": "^1.2.0"
  }
}
