{
  "name": "elm-doctest",
  "version": "0.3.0",
  "description": "doctest for elm",
  "main": "./distribution/index.js",
  "bin": {
    "elm-doctest": "./cli.js"
  },
  "devDependencies": {
    "elm": "^0.18.0",
    "npm-check": "^5.4.0",
    "shelljs": "^0.7.5",
    "standard": "^8.6.0",
    "uglify-js": "^2.7.5"
  },
  "scripts": {
    "prebuild": "npm install && elm-package install --yes",
    "dist": "npm run build && npm run uglify",
    "uglify": "uglifyjs ./distribution/index.js -o tmp.js --define DEBUG=false -c -m && mv ./tmp.js ./distribution/index.js ",
    "build": "elm-make --yes --warn src/Main.elm --output ./distribution/index.js",
    "check": "npm-check -s -p",
    "lint": "standard src/bridge.js",
    "pretest": "npm run build && npm run check && npm run lint",
    "test0": "node ./test/run.js",
    "test": "npm run test0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tshm/elm-doctest.git"
  },
  "author": "Toshihide Satake <2sm@csc.jp>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tshm/elm-doctest/issues"
  },
  "homepage": "https://github.com/tshm/elm-doctest#readme",
  "keywords": [
    "cli",
    "tool",
    "elm",
    "doctest"
  ],
  "dependencies": {
    "chokidar": "^1.6.1",
    "minimist": "^1.2.0"
  }
}
